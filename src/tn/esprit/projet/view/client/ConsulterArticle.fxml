<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="455.0" prefWidth="1200.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="tn.esprit.projet.controler.ConsulterArticleController">
  <children>
    <ScrollPane id="ScrollPane" layoutX="0.0" layoutY="0.0" prefViewportHeight="455.0" prefViewportWidth="1200.0">
      <content>
        <VBox fx:id="vbarticles" focusTraversable="true" prefHeight="455.0" prefWidth="1200.0" spacing="20.0">
          <children>
            <HBox fx:id="hbarticle" minHeight="168.0" prefHeight="206.0" prefWidth="1200.0">
              <children>
                <ImageView fitHeight="206.0" fitWidth="274.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@../../media/LoginLogo.png" />
                  </image>
                </ImageView>
                <VBox prefHeight="100.0" prefWidth="1067.0">
                  <children>
                    <Label text="Titre :">
                      <font>
                        <Font name="System Bold" size="16.0" />
                      </font>
                    </Label>
                    <Label text="Text " />
                    <HBox id="HBox" alignment="CENTER" spacing="5.0">
                      <children>
                        <TextArea prefHeight="32.0" prefWidth="800.0" promptText="Votre commentaire..." wrapText="true">
                          <HBox.margin>
                            <Insets />
                          </HBox.margin>
                        </TextArea>
                        <Button mnemonicParsing="false" prefHeight="32.0" prefWidth="111.0" text="Commenter" />
                      </children>
                      <VBox.margin>
                        <Insets top="135.0" />
                      </VBox.margin>
                    </HBox>
                  </children>
                </VBox>
              </children>
            </HBox>
          </children>
        </VBox>
      </content>
    </ScrollPane>
  </children>
</AnchorPane>
